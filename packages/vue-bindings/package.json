{
  "name": "@sylphx/silk-vue",
  "version": "2.0.6",
  "description": "Vue 3 bindings for Silk - zero-runtime CSS-in-TypeScript with Composition API support",
  "keywords": [
    "css",
    "css-in-js",
    "typescript",
    "vue",
    "vue3",
    "composition-api",
    "reactive",
    "silk"
  ],
  "author": "SylphX Ltd",
  "license": "MIT",
  "homepage": "https://sylphx.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/sylphxltd/silk.git",
    "directory": "packages/vue-bindings"
  },
  "bugs": {
    "url": "https://github.com/sylphxltd/silk/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "types": "./dist/vite.d.ts"
    }
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "bun run build"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "dependencies": {
    "@sylphx/silk": "^2.2.2",
    "@sylphx/silk-vite-plugin": "^2.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "vue": "^3.4.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
