{
  "name": "@sylphx/silk-nuxt",
  "version": "1.0.2",
  "description": "Nuxt 3 module for Silk - Zero-codegen CSS-in-TypeScript",
  "keywords": [
    "silk",
    "nuxt",
    "nuxt-module",
    "css-in-js",
    "typescript",
    "zero-codegen"
  ],
  "author": "SylphX Ltd",
  "license": "MIT",
  "homepage": "https://sylphx.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/SylphxAI/silk.git",
    "directory": "packages/nuxt-module"
  },
  "bugs": {
    "url": "https://github.com/SylphxAI/silk/issues"
  },
  "type": "module",
  "main": "./dist/module.mjs",
